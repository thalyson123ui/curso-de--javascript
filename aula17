// loop while no javaScript//
let contador = 0;

while (contador < 5) {
    console.log("Contagem: " + contador);
    contador++;
}

// Exemplo com strings
var fruta = "banana";

switch (fruta) {
    case "maçã":
        console.log("Fruta: Maçã");
        break;
    case "banana":
        console.log("Fruta: Banana");
        break;
    case "laranja":
        console.log("Fruta: Laranja");
        break;
    default:
        console.log("Fruta desconhecida");
}